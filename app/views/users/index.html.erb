<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if user.username == "guest" %>
        <% next %>
      <% end %>
      <tr>
        <td><%= user.username%></td>
        <td><%= link_to 'View Profile', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
